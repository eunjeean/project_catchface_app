<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JoinActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:text="회원가입"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <EditText
        android:id="@+id/join_id_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" ID"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/check_btn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/join_pw_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" PW"
        android:inputType="textPassword"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/join_id_et"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/join_pwcheck_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" PW CHECK"
        android:inputType="textPassword"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/join_pw_et"
        tools:ignore="TouchTargetSizeCheck" />


    <DatePicker
        android:id="@+id/join_date"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:calendarViewShown="false"
        android:datePickerMode="spinner"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <Button
        android:id="@+id/check_btn"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:backgroundTint="@color/pointOrange"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="중복확인"
        android:textSize="14sp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/join_id_et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/join_id_et"
        app:layout_constraintTop_toTopOf="@+id/join_id_et"
        tools:ignore="TouchTargetSizeCheck" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.35" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <TextView
        android:id="@+id/check_tv"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:padding="2dp"
        android:paddingStart="10dp"
        android:text="비밀번호가 일치합니다"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <Button
        android:id="@+id/wanted2"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/pointOrange"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="회원가입"
        android:textSize="20dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_margin="15dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/applogo" />

    <EditText
        android:id="@+id/phone_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" 핸드폰 번호"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/wanted6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/si_spin"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/number_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" 인증번호작성"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/time_tv"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_et"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/name_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/subbtn_border"
        android:ems="10"
        android:hint=" 이름"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_tv"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:padding="3dp"
        android:text="생년월일"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/guideline11"
        app:layout_constraintTop_toBottomOf="@+id/name_et" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:padding="4dp"
        android:text="성별"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/man_ck"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/guideline11"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <CheckBox
        android:id="@+id/man_ck"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:text="남자"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/woman_ck"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:ignore="TouchTargetSizeCheck" />

    <CheckBox
        android:id="@+id/woman_ck"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:text="여자"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/man_ck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/man_ck"
        app:layout_constraintTop_toTopOf="@+id/man_ck"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/wanted6"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:backgroundTint="@color/pointOrange"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="인증번호"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/phone_et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/phone_et"
        app:layout_constraintTop_toTopOf="@+id/phone_et"
        tools:ignore="TouchTargetSizeCheck" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <TextView
        android:id="@+id/time_tv"
        android:layout_width="90dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:text="2:59"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/number_et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/number_et"
        app:layout_constraintTop_toTopOf="@+id/number_et" />

    <Spinner
        android:id="@+id/si_spin"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/main_city_border"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/dong_spin"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/man_ck"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Spinner
        android:id="@+id/dong_spin"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_margin="10dp"
        android:background="@drawable/main_city_border"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/si_spin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/si_spin"
        app:layout_constraintTop_toTopOf="@+id/si_spin"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>